{
  "name": "loveyu/symfony-workerman-bundle",
  "description": "Workerman模块",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.4",
    "fidry/cpu-core-counter": "^1.2",
    "phpinnacle/buffer": "^1.2",
    "symfony/config": "^7.0",
    "symfony/console": "^7.0",
    "symfony/dependency-injection": "^7.0",
    "symfony/framework-bundle": "^7.0",
    "symfony/http-kernel": "^7.0",
    "symfony/yaml": "^7.1",
    "workerman/crontab": "^1.0.7",
    "workerman/workerman": "^5.1"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\Symfony\\WorkermanBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\Symfony\\WorkermanBundle\\Tests\\": "tests"
    }
  }
}
