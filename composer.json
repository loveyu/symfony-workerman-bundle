{
  "name": "tourze/symfony-workerman-bundle",
  "description": "Workerman模块",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "fidry/cpu-core-counter": "^1.2.0",
    "phpinnacle/buffer": "^1.2.0",
    "symfony/config": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/console": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "workerman/workerman": "^5.1",
    "symfony/yaml": "^6.4 || ^7.1",
    "workerman/crontab": "^1.0.7"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\Symfony\\WorkermanBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\Symfony\\WorkermanBundle\\Tests\\": "tests"
    }
  }
}